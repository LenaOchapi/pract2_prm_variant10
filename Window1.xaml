<Window x:Class="pract2_prm_variant10.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pract2_prm_variant10"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="400">
    <Grid>
        <TextBlock 
           HorizontalAlignment="Left" VerticalAlignment="Top" Margin="147,11,0,0" Height="60" Width="225"><Run Text="Дано число. Вывести цифры, которые "/><LineBreak/><Run Text="повторяются в записи числа больше"/> <LineBreak/><Run Text="одного раза"/></TextBlock>
        <TextBox x:Name="NumberTextBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="FindRepeatedDigitsButton" Content="Find Repeated Digits" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Width="120" Click="FindRepeatedDigitsButton_Click"/>
        <ListBox x:Name="DigitListBox" HorizontalAlignment="Left" Height="200" Margin="10,70,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBlock x:Name="ResultTextBlock" HorizontalAlignment="Left" Margin="10,280,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
