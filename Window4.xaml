<Window x:Class="pract2_prm_variant10.Window4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pract2_prm_variant10"
        mc:Ignorable="d"
        Title="Window4" Height="450" Width="800">
    <Grid>

        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10">
            <TextBox x:Name="InputTextBox" Margin="0,0,0,10" Width="300"/>

            <Button Content="поменять местами" Click="CalculateButton_Click"/>

            <ListBox x:Name="ResultListBox" Margin="0,10,0,0" Width="300" Height="73"/>
        </StackPanel>
        <Label Content="Поменять местами первое простое и первое совершенное числа в массиве" HorizontalAlignment="Left" Margin="67,43,0,0" VerticalAlignment="Top" FontSize="20"/>

    </Grid>
</Window>
